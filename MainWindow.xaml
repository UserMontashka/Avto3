<Window x:Class="Avto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Avto"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="900">
    <Grid>
        <Frame x:Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="5">
            <Image x:Name="Back" Source="Resources/93634.png" Height="35" Width="35" MouseLeftButtonDown="Back_MouseLeftButtonDown"/>
            <Image x:Name="Forward" Source="Resources/93615.png" Height="35"  Width="35"  MouseLeftButtonDown="Forward_MouseLeftButtonDown"/>
            <Image  x:Name="Home" Source="Resources/93624.png"  Height="35" Width="35"  MouseLeftButtonDown="Home_MouseLeftButtonDown"/>
        </StackPanel>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="0"></RowDefinition>
        </Grid.RowDefinitions>
       

    </Grid>
</Window>
